{
  "name": "scss-compiler-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.13.1",
    "react-ace": "^9.1.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "tailwindcss": "^1.5.2"
  },
  "scripts": {
    "start": "npm run tailwind:dev && react-scripts start",
    "build": "npm run tailwind:build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tailwind:build": "NODE_ENV=production tailwind build ./src/styles/tailwind.css -o ./src/styles/index.css",
    "tailwind:dev": "NODE_ENV=development tailwind build ./src/styles/tailwind.css -o ./src/styles/index.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
